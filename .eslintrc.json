{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "plugin:react/recommended"
  ],
  "rules": {
    "react/prop-types": "false",
    "arrow-body-style": 0,
    "consistent-return": "off",
    "no-console": "off",
    "import/prefer-default-export": "off"
  }
}